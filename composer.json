{
  "name": "h-o-sein/cpanel-whm",
  "description": "Laravel cPanel/WHM API Package.",
  "keywords": ["framework", "laravel","cPanel","WHM","cPanel API"],
  "license": "MIT",
  "authors": [
    {
      "name": "Mochamad Gufron",
      "email": "mgufronefendi@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "composer require am2tec/cpanel-php":"*",
    "guzzlehttp/guzzle" : "*"
  },
  "autoload": {
    "classmap": [
      "src/migrations"
    ],
    "psr-0": {
      "Gufy\\CpanelWhm\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gufy\\CpanelWhm\\CpanelWhmServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
